{
    "manifest_version": 2,
    "name": "Dojo Text Modifier",
    "version": "1.0",
    "permissions": [
      "*://tms.artmuseums.harvard.edu/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://tms.artmuseums.harvard.edu/*"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ]
}