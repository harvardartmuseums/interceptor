{
    "manifest_version": 3,
    "name": "TMS Text Interceptor",
    "version": "1.0",
    "host_permissions": [
      "*://tms.artmuseums.harvard.edu/*"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
    "content_scripts": [
      {
        "matches": ["*://tms.artmuseums.harvard.edu/*"],
        "js": ["content.js"],
        "run_at": "document_start"
      }
    ]
}